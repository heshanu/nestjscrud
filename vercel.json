{
    "version": 2,
    "builds": [
      {
        "src": "dist/vercel.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "api/v1/(.*)",
        "dest": "dist/vercel.ts/api/v1/(.*)",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }